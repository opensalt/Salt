{
    "name": "opensalt/opensalt",
    "license": "MIT",
    "type": "project",
    "description": "A competency framework management system that works with the CASE format",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "DoctrineMigrations\\": "migrations/",
            "LocalApp\\": "local-src/"
        },
        "exclude-from-classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Codeception\\Module\\": "tests/_support/Module"
        },
        "classmap": [
            "tests/_behat/Context.php"
        ]
    },
    "repositories": [
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0.0",
        "ext-PDO": "*",
        "ext-bz2": "*",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "antishov/doctrine-extensions-bundle": "^1.4.3",
        "colinodell/json5": "^2.1",
        "composer/package-versions-deprecated": "^1.11",
        "csa/guzzle-bundle": "dev-master#9858ec2472af768a86991ec0cf760208d7df73fb as 3.2.0.1",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.0.2",
        "doctrine/migrations": "^3.0.2",
        "doctrine/orm": "^2.8",
        "firebase/php-jwt": "^5.2",
        "gbprod/uuid-normalizer": "^1.1",
        "google/recaptcha": "~1.1",
        "jms/serializer-bundle": "^3.3.0",
        "kamermans/guzzle-oauth2-subscriber": "^1.0.6",
        "laminas/laminas-code": "^4.0",
        "laminas/laminas-escaper": "^2.2",
        "league/csv": "^9.6",
        "league/flysystem-aws-s3-v3": "^2.0",
        "league/oauth2-github": "^2.0.0",
        "league/uri": "^6.0",
        "milo/github-api": "^1.4",
        "namshi/cuzzle": "^2.0",
        "nelmio/cors-bundle": "^2.0",
        "nelmio/security-bundle": "^2.7",
        "phpoffice/phpspreadsheet": "~1.17.0",
        "phpoffice/phpword": "^0.18.0",
        "qandidate/toggle-bundle": "^1.3.1",
        "ramsey/uuid": "^4.0",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^6.0",
        "swaggest/json-schema": "^0.12.20",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.8.4",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/mercure-bundle": "^0.3.0",
        "symfony/mime": "5.2.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/process": "5.2.*",
        "symfony/profiler-pack": "*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/security-csrf": "5.2.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "5.2.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/yaml": "5.2.*",
        "tecnickcom/tcpdf": "^6.2",
        "tetranz/select2entity-bundle": "^3.0",
        "twig/string-extra": "^3.1",
        "twig/twig": "^3.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/debug-pack": "*",
        "symfony/test-pack": "*",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "5.2.*",
        "jetbrains/phpstorm-attributes": "^1.0",

        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0",
	"codeception/module-doctrine2": "^1.0",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.0",
        "codeception/module-sequence": "^1.0",
        "codeception/module-symfony": "^2.0",
        "codeception/module-webdriver": "^1.0",
            "codeception/specify": "^1.1",
            "codeception/verify": "^2.0",
            "softcreatr/jsonpath": "~0.4",
            "league/factory-muffin": "^3.3",
            "league/factory-muffin-faker": "^2.0",
            "phpunit/phpunit": "^9.0",
            "vlucas/phpdotenv": "^5.0",

        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",

        "vimeo/psalm": "^4.6",
        "psalm/plugin-symfony": "^2.1",
        "weirdan/doctrine-psalm-plugin": "^1.0",

        "symplify/easy-coding-standard": "^9.0",
        "escapestudios/symfony2-coding-standard": "^3.1",
        "ericmartel/codeception-email-mailtrap": "^1.0"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.2.*"
        }
    }
}
