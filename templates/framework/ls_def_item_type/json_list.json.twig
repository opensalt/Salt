{% extends 'layout.html.twig' %}

{% block body %}
    {
        "results": [
            {% for obj in results %}
                {% if not loop.first %},{% endif %}
                { "id": {{ obj.id|json_encode|raw }}, "text": {{ obj.title|json_encode|raw }} }
            {% endfor %}
        ],
        "more": {{ more|json_encode|raw }}
    }
{% endblock %}

